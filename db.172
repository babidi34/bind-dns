;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	serveur-test.linux.man root.linux.man (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	serveur-test.
40.16.16	IN	PTR	serveur-test.linux.man

